{
	"name": "blogger-template",
	"version": "1.0.0",
	"description": "blogger",
	"main": "gulpfile.js",
	"scripts": {
		"build": "npx tailwindcss -i ./src/assets/styles/tailwind.css -o ./src/assets/styles/styles.min.css --minify && gulp build:production",
		"gulp": "gulp",
		"dev": "npx tailwindcss -i ./src/assets/styles/tailwind.css -o ./src/assets/styles/styles.min.css",
		"webpack:build": "webpack --mode=production",
		"webpack:dev": "webpack --mode=development",
		"css:build": "tailwindcss -i src/css/tailwind.css -o dist/css/tailwind.css",
		"css:watch": "tailwindcss -i ./src/assets/styles/tailwind.css -o dist/css/tailwind.css --watch"
	},
